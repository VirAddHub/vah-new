{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://vah-api-staging.onrender.com/api/:path*"
        }
    ],
    "env": {
        "NEXT_PUBLIC_API_URL": "https://vah-api-staging.onrender.com",
        "NEXT_PUBLIC_API_BASE_URL": "https://vah-api-staging.onrender.com",
        "NEXT_PUBLIC_APP_URL": "https://vah-frontend-final.vercel.app",
        "BACKEND_API_ORIGIN": "https://vah-api-staging.onrender.com",
        "NEXT_PUBLIC_BACKEND_API_ORIGIN": "https://vah-api-staging.onrender.com"
    }
}