{
    "name": "vah",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": "20.x"
    },
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "start:backend": "npm --workspace apps/backend run start",
        "start:frontend": "npm --workspace apps/frontend run dev",
        "build": "npm run build --workspaces --if-present",
        "build:backend": "npm run -w apps/backend build",
        "dev:backend": "node apps/backend/dist/server/index.js",
        "install:all": "npm install --workspaces",
        "test": "npm run test --workspaces --if-present"
    },
    "devDependencies": {
        "turbo": "^1.10.0"
    },
    "resolutions": {
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0"
    },
    "overrides": {
        "dompurify": "^3.2.7",
        "marked": "^16.4.0",
        "elliptic": "^6.6.1",
        "sha.js": "^2.4.12",
        "pbkdf2": "^3.1.2",
        "cipher-base": "^1.0.4",
        "browserify-sign": "^4.2.1",
        "@babel/traverse": "^7.23.2",
        "brace-expansion": "^2.0.1",
        "braces": "^3.0.3",
        "serialize-javascript": "^6.0.1",
        "prismjs": "^1.30.0",
        "webpack": "^5.93.0"
    },
    "dependencies": {
        "@microsoft/microsoft-graph-client": "^3.0.7",
        "dompurify": "^3.2.7",
        "drizzle-kit": "^0.31.5",
        "isomorphic-fetch": "^3.0.0",
        "marked": "^16.4.0"
    }
}